{
  "name": "ai-wizard-duel-convex",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "local-dev": "concurrently \"next dev --turbopack\" \"convex dev\"",
    "local-start": "concurrently \"next start --turbopack\" \"convex dev\"",
    "build-deploy-start": "npm run build && npm run deploy && npm run start",
    "dev": "npm run set-dev-envs && npm run local-dev",
    "emulate": "npm run set-emulate-envs && docker-compose up -d && npm run local-dev",
    "set-dev-envs": "cp .env.dev .env.local",
    "set-emulate-envs": "cp .env.emulate .env.local",
    "deploy": "convex deploy",
    "dashboard": "convex dashboard",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix && prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:debug": "vitest --inspect-brk --no-file-parallelism",
    "test:coverage": "vitest run --coverage --coverage.reporter=text",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:emulate": "npm run set-emulate-envs && start-server-and-test emulate http://localhost:3000 cypress:run",
    "cypress:emulate:open": "npm run set-emulate-envs && start-server-and-test emulate http://localhost:3000 cypress:open",
    "clear-emulate-db": "docker compose down -v && docker compose up -d && npm run new-emulate-creds",
    "new-emulate-creds": "sed -i '' \"/^CONVEX_SELF_HOSTED_ADMIN_KEY=/d\" .env.emulate && echo \"CONVEX_SELF_HOSTED_ADMIN_KEY='$(docker compose exec -T backend ./generate_admin_key.sh)'\" >> .env.emulate"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.28.1",
    "@clerk/themes": "^2.4.4",
    "@fal-ai/serverless-client": "^0.15.0",
    "@genkit-ai/core": "^1.15.5",
    "@genkit-ai/googleai": "^1.15.5",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.25.4",
    "genkit": "1.15.5",
    "lucide-react": "^0.536.0",
    "next": "15.4.5",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@edge-runtime/vm": "^5.0.0",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.19.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.1.0",
    "convex-test": "^0.0.38",
    "cypress": "^13.17.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "start-server-and-test": "^2.0.8",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
