{
  "name": "ai-wizard-duel-convex",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"convex dev\"",
    "start": "concurrently \"next start\" \"npx convex dev\"",
    "emulate": "concurrently \"next dev\" \"npx convex dev --local\"",
    "build-deploy-start": "npm run build && npm run deploy && npm run start",
    "deploy": "convex deploy",
    "dashboard": "convex dashboard",
    "build": "next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix && prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:debug": "vitest --inspect-brk --no-file-parallelism",
    "test:coverage": "vitest run --coverage --coverage.reporter=text",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:emulate": "npm run set-emulate-envs && start-server-and-test emulate http://localhost:3000 cypress:run",
    "cypress:emulate:open": "npm run set-emulate-envs && start-server-and-test emulate http://localhost:3000 cypress:open"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.32.2",
    "@clerk/themes": "^2.4.21",
    "@fal-ai/serverless-client": "^0.15.0",
    "@genkit-ai/core": "^1.19.3",
    "@genkit-ai/google-genai": "^1.21.0",
    "@genkit-ai/googleai": "^1.19.3",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.27.4",
    "genkit": "1.21.0",
    "lucide-react": "^0.545.0",
    "next": "15.5.4",
    "next-themes": "^0.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@edge-runtime/vm": "^5.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.19.19",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.1",
    "convex-test": "^0.0.38",
    "cypress": "^15.2.0",
    "eslint": "^9.36.0",
    "eslint-config-next": "15.5.4",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "start-server-and-test": "^2.1.0",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
